def Dice_face(n):
    # یک دیکشنری برای اعداد تعریف میکنیم که اعدای روبروی انها چیست
    Dice_faces = {
        1: 6,
        2: 5,
        3: 4,
        4: 3,
        5: 2,
        6: 1
    }
     # عدد مقابل را به کاربر برگردانیم
    return Dice_faces[n]
# ورودی را از کاربر بگیرم
n = int(input()) 
# دریافت ورودی از کاربر بگیریم
# خروجی
D = Dice_face(n) 
# عدد مقابل چیست
print(D) 
# چاپ عدد مقابل
